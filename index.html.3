<script type="text/javascript" language="JavaScript">
	var arrayOfData = new Array();

	function geoCoding() {
		jQuery("#criterias\\.pointInfo\\[0\\]\\.name").googleAutoComplete("criterias\\.pointInfo\\[0\\]")
		jQuery("#criterias\\.pointInfo\\[1\\]\\.name").googleAutoComplete("criterias\\.pointInfo\\[1\\]")

	}
	function init() {
		updateInDateOrOutDate('outDate');
		updateInDateOrOutDate('inDate');
		geoCoding()

		jQuery("#submit-search").click(function(event) {
			sendSearchForm();
			event.preventDefault();
		})

		jQuery("#show_on_map_0").click(function() {
			jQuery("#criterias\\.pointInfo\\[0\\]\\.name").focus()
		})
		jQuery("#show_on_map_1").click(function() {
			jQuery("#criterias\\.pointInfo\\[1\\]\\.name").focus()
		})
	}

	jQuery(document).ready(function() {
		init()
		initDatePicker("#criterias\\.byDayBeginDate", "#criterias\\.byDayEndDate", "havas", "3");
	})

	function initialize() {
		initAutocomplete(0);
		initAutocomplete(1);
	}

	function initAutocomplete(inputId) {
		var input = document.getElementById('criterias.pointInfo[' + inputId + '].name');
		var autocomplete = new google.maps.places.Autocomplete(input);

		autocomplete.setTypes([ 'geocode' ]);

		/*Evènement permettant de récupérer la ville sélectionnée au clavier ou à la souris...!!*/
		google.maps.event.addListener(autocomplete, 'place_changed', function() {
			var place = autocomplete.getPlace();
			var latitude = place.geometry.location.lat();
			var inputLatitude = document.getElementById('criterias.pointInfo[' + inputId + '].latitude');
			inputLatitude.value = latitude;
			var longitude = place.geometry.location.lng();
			var inputLongitude = document.getElementById('criterias.pointInfo[' + inputId + '].longitude');
			inputLongitude.value = longitude;
			var inputType = document.getElementById('criterias.pointInfo[' + inputId + '].type');
			inputType.value = "GEO";
			if (inputId == 0 && document.getElementById('criterias.pointInfo[1].name').value == "") {
				/* Si la ville d'arrivée est vide et qu'on fait une recherche sur le champs de départ alors ville départ = ville arrivée */
				document.getElementById('criterias.pointInfo[1].name').value = document.getElementById('criterias.pointInfo[0].name').value;
				document.getElementById('criterias.pointInfo[1].latitude').value = inputLatitude.value;
				document.getElementById('criterias.pointInfo[1].longitude').value = inputLongitude.value;
				document.getElementById('criterias.pointInfo[1].type').value = inputType.value;
			}
		});
	}

	//google.maps.event.addDomListener(window, 'load', initialize);
</script>


